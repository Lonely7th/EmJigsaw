<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.em.jigsaw">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission
        android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"
        tools:ignore="ProtectedPermissions" />

    <application
        android:name=".BaseApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.linchaolong.android.imagepicker.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity android:name=".activity.JigsawViewActivity" />
        <activity android:name=".activity.AddJigsawActivity" />
        <activity android:name=".activity.PersonalActivity" />
        <activity android:name=".activity.ReleaseListActivity" />
        <activity android:name=".activity.StarListActivity" />
        <activity android:name=".activity.ChangeUserInfoActivity" />
        <activity android:name=".activity.MyStarListActivity" />
        <activity android:name=".activity.MyReleaseListActivity"></activity>
    </application>

</manifest>